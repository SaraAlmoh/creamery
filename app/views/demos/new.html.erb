




<%= form_for(:demo, url: demos_path) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email, :class => "form-control", :type => "number" %>
    
    <%= f.label :password %>
    <%= f.password_field :password, :class => "form-control", :type => "number" %>
    
    <%= f.submit "Login" %>
<% end %>